{
    "name": "ipointchris/vault-php",
    "type": "library",
    "license": "MIT",
    "homepage": "https://ipointsolutions.net",
    "description": "A PHP client library for 'Vault by HashiCorp'",
    "require": {
        "ext-json": "*",
        "guzzlehttp/psr7": "^2.6",
        "php-http/message":"*",
        "php": "^5.6 || ^7.0 || ^8.0",
        "php-http/httplug": ">=1.1.0",
        "php-http/curl-client": ">=2.3.0",
        "tuupola/http-factory": ">=1.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0.0"
    },
    "authors": [
        {
            "name": "Chris Kuhn",
            "email": "Chris@ipointsolutions.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "VaultPHP\\": "vendor\\VaultPHP\\"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\VaultPHP\\": "tests\\VaultPHP\\"
        }
    },
    "scripts": {
        "test": "php ./vendor/bin/phpunit --configuration ./phpunit.xml.dist"
    }
}
